@Library('mylibrary')_
node('built-in')
{
    stage('continous download_Master')
    {
        cicd1.newGit("https://github.com/nagaprasady/maven.git")
    }
    stage('continous build_Master')
    {
        cicd1.mavenbuild()
    }
    stage('continous deploy_Master')
    {
        cicd1.tomcatdeploy("sharedlibrary","172.31.42.144","newtest")
    }
    stage('continous test_Master')
     {  cicd1.newGit("https://github.com/nagaprasady/FunctionalTesting.git")
        cicd1.runselenium("sharedlibrary")
    }
    stage('continous delivery_Master')
    {
        cicd1.tomcatdeploy("sharedlibrary","172.31.47.170","prodtest")
    }
}
